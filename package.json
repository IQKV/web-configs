{
  "name": "@iqkv/web-config",
  "private": false,
  "description": "Common configurations for building web apps at IQKV.",
  "version": "0.23.9",
  "scripts": {
    "build": "lerna run build",
    "lint": "prettier --check . && eslint --output-file checkstyle.xml --format checkstyle .",
    "lint:dev": "eslint .",
    "lint:fix": "eslint . --fix",
    "release": "./release.sh",
    "prepare": "husky install",
    "prettier": "prettier --write ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/IQKV/web-config.git"
  },
  "author": "Dima Denysenko",
  "license": "Apache",
  "bugs": {
    "url": "https://github.com/IQKV/web-config/issues"
  },
  "homepage": "https://github.com/IQKV/web-config",
  "devDependencies": {
    "@commitlint/cli": "^18.4.3",
    "@commitlint/config-conventional": "^18.4.3",
    "@iqkv/prettier-config": "workspace:^",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.53.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-jest": "^27.6.0",
    "husky": "^8.0.3",
    "lerna": "^8.0.1",
    "lint-staged": "^15.2.0",
    "nx": "^17.2.6",
    "prettier": "^3.0.3"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "prettier": "@iqkv/prettier-config"
}
